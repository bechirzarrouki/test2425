<h1>List of authors<h1>
<table border="1">
<tr>
<th>id</th>
<th>nom</th>
<th>date</th>
<th>action</th>
</tr>
{% for i in parties %}
<tr>
    <td>
    {{i.id}}
    </td>
    <td>
    {{i.nom}}
    </td>
    <td>
    {{i.PartieDate|date('d-m-Y')}}
    </td>
    <td>
    <a href="{{ path('app2_partie', { 'id': i.id }) }}">edit </a>
    <a href="{{ path('app3_partie', { 'id': i.id }) }}">remove </a>
    </td>
</tr>
{% endfor %}
</table>